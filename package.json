{
  "name": "pixelarium",
  "description": "The simplest image hosting ever.",
  "module": "server.ts",
  "type": "module",
  "scripts": {
    "start": "bun run ./src/server.ts",
    "dev": "env-cmd -f .env.dev bun run --watch ./src/server.ts",
    "staging": "env-cmd -f .env.staging bun run --watch ./src/server.ts",
    "test:dev": "env-cmd -f .env.dev bun test --watch --timeout 8000",
    "test:staging": "env-cmd -f .env.staging bun test --watch --timeout 8000",
    "lint": "bun eslint ./src --ext .ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "bun-types": "latest",
    "eslint": "^8.54.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@prisma/extension-accelerate": "^0.6.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.4.4",
    "@types/express-rate-limit": "^6.0.0",
    "@types/winston": "^2.4.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.3",
    "express-rate-limit": "^7.1.5",
    "googleapis": "^129.0.0",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.6.0",
    "winston": "^3.11.0"
  }
}
